{
  "name": "card-slam-fantasy-hoops",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b ./tsconfig.app.json && vite build",
    "lint": "eslint .",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,scss}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,css,scss}'",
    "typecheck": "tsc -p tsconfig.ci.json --noEmit",
    "typecheck:all": "tsc -b",
    "preview": "vite preview",
    "test": "jest",
    "test:quiet": "NODE_OPTIONS='--no-warnings' yarn jest --silent",
    "setup:firebase": "node src/scripts/setupEnv.js",
    "validate:firebase": "node src/scripts/validateConfig.js",
    "test:firebase": "node src/scripts/testConnectivity.js",
    "clean:cache": "rm -rf node_modules/.vite",
    "prepare": "husky",
    "commitlint": "commitlint"
  },
  "dependencies": {
    "@firebase/app": "^0.11.5",
    "firebase": "^10.6.0",
    "firebase-admin": "^11.11.1",
    "firebase-functions": "^4.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.22.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/identity-obj-proxy": "^3",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^16.5.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.2.4",
    "supports-color": "^10.0.0",
    "ts-jest": "^29.3.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-node": "^3.1.2"
  },
  "packageManager": "yarn@4.9.1",
  "dependenciesMeta": {
    "firebase": {
      "built": false
    }
  }
}
